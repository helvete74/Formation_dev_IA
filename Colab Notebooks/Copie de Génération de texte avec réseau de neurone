 = F.one_hot(xs, num_classes=num_chars).float() # input to the network: one-hot encoding\n","  logits = xenc @ W # predict log-counts\n","  counts = logits.exp() # counts, equivalent to N\n","  probs = counts / counts.sum(1, keepdims=True) # probabilities for next character\n","  loss = -probs[torch.arange(num), ys].log().mean()\n","  #print(loss.item())\n","  perplexity = torch.exp(loss)\n","  print(f'Perplexité: {perplexity:.2f}')\n","\n","  # backward pass\n","  W.grad = None # set to zero the gradient\n","  loss.backward()\n","\n","  # update\n","  W.data += -50 * W.grad"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"04u97uCiVpLj","executionInfo":{"status":"ok","timestamp":1716921608514,"user_tz":-120,"elapsed":53808,"user":{"displayName":"Florian Laurent","userId":"06835142167550570363"}},"outputId":"6848c011-aa05-44fd-c3a7-863f748c585a"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["  0%|          | 1/500 [00:00<01:39,  5.04it/s]"]},{"output_type":"stream","name":"stdout","text":["Perplexité: 12.34\n","Perplexité: 12.33\n"]},{"output_type":"stream","name":"stderr","text":["  1%|          | 3/500 [00:00<01:26,  5.77it/s]"]},{"output_type":"stream","name":"stdout","text":["Perplexité: 12.33\n","Perplexité: 12.32\n"]},{"output_type":"stream","name":"stderr","text":["  1%|          | 5/500 [00:00<01:22,  6.00it/s]"]},{"output_type":"stream","name":"stdout","text":["Perplexité: 12.32\n","Perplexité: 12.31\n"]},{"output_type":"stream","name":"stderr","text":["  1%|▏         | 7/500 [00:01<01:16,  6.41it/s]"]},{"output_type":"stream","name":"stdout","text":["Perplexité: 12.31\n","Perplexité: 12.31\n","Perplexité: 12.30\n"]},{"output_type":"stream","name":"stderr","text":["  2%|▏         | 11/500 [00:01<00:56,  8.71it/s]"]},{"output_type":"stream","name":"stdout","text":["Perplexité: 12.30\n","Perplexité: 12.29\n","Perplexité: 12.29\n"]},{"output_type":"stream","name":"stderr","text":["  3%|▎         | 14/500 [0