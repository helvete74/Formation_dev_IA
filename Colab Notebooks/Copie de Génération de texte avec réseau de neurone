rue)\n","\n","    ix = torch.multinomial(p, num_samples=1, replacement=True).item()\n","    out.append(index_to_char[ix])\n","    if ix == 0:\n","      break\n","  print(''.join(out))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"QhP_N2yDVpG1","executionInfo":{"status":"ok","timestamp":1716921490569,"user_tz":-120,"elapsed":189,"user":{"displayName":"Florian Laurent","userId":"06835142167550570363"}},"outputId":"6e1f7ccd-7da8-43a5-e6b2-dc5306349309"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["jorifea.\n","fannyamvilocuriene.\n","emath.\n","pqüwesorehane.\n","pùtily.\n"]}]}]}